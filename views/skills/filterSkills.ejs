


<form action="/skills" method="GET" class="filter skillFilter">
  <br>  
  <label for="skillsSearch">Choose Skills To Search : </label>
  <br>
    <select id="skillsSearch" class="form-select select2Student" multiple name="skills">
      <% for (var i=0; i < items.length; i++) { %>
        <option value="<%= items[i].name %>"><%=items[i].name%></option>
        <% } %>
            </select>
      
                <button type="submit" class="btn btn-secondary" id="button-filter" > Search</button>
    </form>
    